		(= (max_queue r3) 1000.0)
		(active ter2 r7n)
		(= (tokenvalue r6 ter2) 20.0)
		(= (tokenvalue r5 ter2) 10.0)
		(= (queue r2) 40.0)
		(availableflow r8 r10 ter3)
		(= (queue r3) 100.0)
		(= (tokenvalue r8 ter3) 20.0)
