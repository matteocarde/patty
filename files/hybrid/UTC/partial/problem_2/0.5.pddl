	)
	(:init
		(availableflow r6 r7s ter2)
		(= (maxtoken ter3) 20.0)
		(availableflow r7n r9 ter2)
		(active ter2 r5)
		(availableflow r3 r5 ter1)
		(= (queue r9) 0.0)
		(= (tokenvalue r7s ter3) 10.0)
		(= (max_queue r5) 70.0)
		(active ter3 r8)
		(= (flow r8 r7n ter3) 2.0)
		(= (tokenvalue r6 ter2) 20.0)
		(availableflow r2 r5 ter1)
		(= (flow r3 r5 ter1) 2.0)
		(= (maxtoken ter1) 30.0)
		(= (queue r4) 30.0)
		(= (flow r6 r9 ter2) 2.0)
		(= (max_queue r7s) 50.0)
		(availableflow r8 r7n ter3)
		(= (tokenvalue r8 ter3) 20.0)
		(= (flow r2 r5 ter1) 2.0)
		(active ter2 r7n)
