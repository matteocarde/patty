		(= (token ter3) 10.0)
		(= (flow r5 r9 ter2) 2.0)
		(= (queue r1) 60.0)
		(availableflow r5 r7s ter2)
		(= (max_queue r2) 1000.0)
	)
	(:goal
			(and
				(< (queue r1) 10.0)
				(< (queue r2) 10.0)
				(< (queue r3) 10.0)
			)
	)
